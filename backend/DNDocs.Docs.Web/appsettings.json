{
  "DSettings": {
    "DFileSystemOptions": {
      "InfrastructureFolderOSPath": "", // on deploy
      "AppDbSqliteFileName": "appdb.sqlite",
      "LogDbSqliteFileName": "log.sqlite",
      "SiteDbSqliteFileName": "site.sqlite",
      "VarSiteDbSqliteFileName": "varsite.sqlite"
    },

    "Strings": {
      "UrlNugetProjectGenerate": "https://dndocs.com/i/nuget/{0}/{1}", // 0 - package name, 1 - package version
      "UrlProjectSingletonFormat": "https://docs.dndocs.com/s/{0}{1}", // 0 - urlprefix 1 - path
      "UrlProjectVersionFormat": "https://docs.dndocs.com/v/{0}/{1}{2}", // 0 - urlprefix 1- version 2 -path
      "UrlProjectNugetOrgFormat": "https://docs.dndocs.com/n/{0}/{1}{2}", // 0 - nugetPackageName, 1 - nuget package version 2 -path
      "UrlDDocs": "https://docs.dndocs.com{0}" // 0 - some relative path
    },

    "TimespanGenerateSitemapPeriod": "1.0:0:0",
    "TimeSpanSaveMetrics": "0.0:0:30",
    "FlushAllLogsTimeSpan": "0.0:0:10",
    "MemoryCacheMaxSizeMB": 6144,

    "DDocsApiKey": "" // on deploy
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "DNDocs": "Trace",
      "Vinca": "Trace"
    },
    "Console": {
      "LogLevel": {
        "Default": "Error"
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "", // on deploy
        "KeyPath": "" // on deploy
      }
    },
    "Endpoints": {
      "Https": {
        "Url": "https:" // on deploy
      }
    }
  }
}
